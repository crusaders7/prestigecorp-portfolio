{
    "functions": {
        "projects/newspaperscraper/api/*.py": {
            "runtime": "python3.8"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/projects/newspaperscraper/api/$1"
        }
    ],
    "build": {
        "env": {
            "PYTHONPATH": "."
        }
    }
}