{
  "version": 2,
  "builds": [
    {
      "src": "api/search.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/search",
      "dest": "/api/search.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}